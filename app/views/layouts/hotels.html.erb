    name="description">
    <meta content=
    "width=device-width, initial-scale=1.0, user-scalable=no, minimal-ui" name=
    "viewport">
    <link data-turbolinks-track="true" href=
    "/public_assets/hotels.css" media="all"
    rel="stylesheet">
